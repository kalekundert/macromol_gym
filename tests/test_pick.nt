test_count_atoms:
  -
    atoms:
      > subchain occ
      > A        1
    expected:
      > subchain occ
      > A        1
  -
    atoms:
      > subchain occ
      > A        1
      > B        1
    expected:
      > subchain occ
      > A        1
      > B        1
  -
    atoms:
      > subchain occ
      > A        1
      > A        1
      > A        1
      > B        1
      > B        1
    expected:
      > subchain occ
      > A        3
      > B        2
  -
    atoms:
      > subchain occ
      > A        0.5
      > A        0.5
      > A        0.5
      > B        1
      > B        1
    expected:
      > subchain occ
      > A        1.5
      > B        2
  -
    id: 1aa5
    # I chose this structure because it's relatively small and has lots of 
    # partial occupancy.  I calculated the total occupancy of subchains A-B in 
    # PyMOL.  The rest I calculated by hand.
    atoms: 1aa5.cif.gz
    expected:
      > subchain occ
      > A        131.74
      > B        132.00
      > C         45.75
      > D         46.00
      > E          8.00
      > F          1.00
      > G          1.00
      > H          1.00
      > I          1.00
      > J         29.10
      > K         16.29

